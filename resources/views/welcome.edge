<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>AdonisJS - A fully featured web framework for Node.js</title>
  <link href="https://fonts.googleapis.com/css?family=Poppins:400,500&display=swap" rel="stylesheet">
  
  @if(app.nodeEnvironment === 'development')
    <script type="module" src="http://localhost:5173/@vite/client"></script>
    <script type="module" src="http://localhost:5173/assets/main.ts"></script>
  @endif

  @if(app.nodeEnvironment === 'production')
    @if(asset('main.ts').css)
      <link rel="stylesheet" href="/assets/{{ asset('main.ts').css }}">
    @endif
    <script type="module" src="/assets/{{ asset('main.ts').file }}"></script>
  @endif

</head>
<body>

</body>
</html>
